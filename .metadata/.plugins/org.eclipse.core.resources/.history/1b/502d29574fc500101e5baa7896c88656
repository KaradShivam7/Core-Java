
      //wap to pront the occurance of any number

package array_questions;

public class ninth {

	public static void main(String[] args) {
		int arr[]= {10,1,33,1,54,1,5,1,3,1,0,1,7,7,9,1};

		int count=0;
		for(int i=0;i<arr.length;i++) {
			if(arr[i]==1) {
				count++;
			}
			
			System.out.println("occurance of 1 ="+ count);
		}
	}

}
